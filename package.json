{
  "name": "wa-baileys-dashboard",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:prod": "tsx ./scripts/migrate.ts",
    "db:seed": "tsx ./scripts/seed.ts",
    "format": "prettier --write . && eslint --fix .",
    "docker:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker compose -f docker-compose.yml -f docker-compose.dev.yml logs -f app",
    "docker:prod": "docker compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "docker:prod:down": "docker compose -f docker-compose.yml -f docker-compose.prod.yml down",
    "docker:prod:logs": "docker compose -f docker-compose.yml -f docker-compose.prod.yml logs -f app",
    "docker:prod:seed": "docker compose -f docker-compose.yml -f docker-compose.prod.yml exec app npm run db:seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/cors": "^11.2.0",
    "@fastify/env": "^5.0.3",
    "@fastify/formbody": "^8.0.2",
    "@fastify/secure-session": "^8.3.0",
    "@fastify/sensible": "^6.0.4",
    "@fastify/static": "^9.0.0",
    "@fastify/type-provider-typebox": "^6.1.0",
    "@fastify/view": "^11.1.1",
    "@fastify/websocket": "^11.2.0",
    "@sinclair/typebox": "^0.34.47",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "argon2": "^0.44.0",
    "bullmq": "^5.66.5",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "ejs": "^3.1.10",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
    "ioredis": "^5.9.1",
    "postgres": "^3.4.8",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^25.0.6",
    "@types/qrcode": "^1.5.6",
    "@types/ws": "^8.18.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.7.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
